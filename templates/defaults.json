{
  "defaults": [
    {
      "name": "irving/header-wrapper",
      "children": [
        {
          "name": "irving/site-theme-provider",
          "config": {
            "selector": "templates.defaults.irving/header-wrapper.template"
          },
          "children": [
            {
              "name": "irving/template-part",
              "use_context": {
                "irving/site-theme": "slug"
              }
            }
          ]
        }
      ]
    },
    {
      "name": "irving/body-wrapper"
    },
    {
      "name": "irving/footer-wrapper",
      "children": [
        {
          "name": "template-parts/footer-default"
        },
        {
          "name": "irving/site-theme-provider",
          "config": {
            "selector": "templates.defaults.irving/footer-wrapper.enable_copyright_template"
          },
          "children": [
            {
              "name": "irving/template-part",
              "use_context": {
                "irving/site-theme": "slug"
              }
            }
          ]
        }
      ]
    }
  ]
}
